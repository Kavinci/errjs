{
	"plugins": ["plugins/markdown", "plugins/summarize"],
	"recurseDepth": 10,
	"source": {
		"include": ["src/", "tests/"],
		"includePattern": ".+\\.js(doc|x)?$",
		"excludePattern": "(^|\\/|\\\\)_"
	},
	"sourceType": "module",
	"tags": {
		"allowUnknownTags": true,
		"dictionaries": ["jsdoc", "closure"]
	},
	"templates": {
		"cleverLinks": false,
		"monospaceLinks": false,
		"defaults": {
			"outputSourceFiles": false
		}
	},
	"opts": {
		"encoding": "utf8",
		"destination": "./docs/",
		"recurse": true,
		"readme": "README.md"
	}
}
